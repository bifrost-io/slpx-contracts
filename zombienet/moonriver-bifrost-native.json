{
	"settings": {
		"provider": "native",
		"timeout": 1800
	},
	"relaychain": {
		"chain": "kusama-local",
		"default_command": "polkadot-v0.9.42-fast-sudo",
		"default_args": ["-lparachain=debug", "-lxcm=trace", "--state-pruning=archive", "--no-hardware-benchmarks"],
		"nodes": [
			{
				"name": "alice",
				"validator": true,
				"env": [
					{
						"name": "RUST_LOG",
						"value": "trace"
					}
				],
				"ws_port": "{{RELAY_WS_PORT}}",
				"rpc_port": "{{RELAY_RPC_PORT}}"
			},
			{
				"name": "bob",
				"validator": true,
				"env": [
					{
						"name": "RUST_LOG",
						"value": "trace"
					}
				]
			},
			{
				"name": "charlie",
				"validator": true,
				"env": [
					{
						"name": "RUST_LOG",
						"value": "trace"
					}
				]
			}
		]
	},
	"parachains": [
		{
			"cumulus_based": true,
			"id": 2023,
			"chain": "moonriver-local",
			"default_args": [
				"--trie-cache-size=0",
				"--state-pruning=archive",
				"--no-hardware-benchmarks",
				"--unsafe-ws-external",
				"--unsafe-rpc-external",
				"--rpc-cors=all"
			],
			"collators": [
				{
					"command": "moonbeam",
					"name": "alith",
					"ws_port": "{{PARACHAIN_WS_PORT}}",
					"rpc_port": "{{PARACHAIN_RPC_PORT}}",
					"args": ["-lparachain=debug", "-lxcm=trace"]
				}
			]
		},
		{
			"cumulus_based": true,
			"id": 2001,
			"chain": "bifrost-kusama-local",
			"default_args": [
				"--trie-cache-size=0",
				"--state-pruning=archive",
				"--no-hardware-benchmarks",
				"--unsafe-ws-external",
				"--unsafe-rpc-external",
				"--rpc-cors=all"
			],
			"collators": [
				{
					"command": "bifrost-test",
					"name": "alice",
					"ws_port": "{{BIFROST_WS_PORT}}",
					"rpc_port": "{{BIFROST_RPC_PORT}}",
					"args": ["-lparachain=debug,slpx=info", "-lxcm=trace","-llog=info"]
				}
			]
		}
	],
	"hrmp_channels": [
		{
			"sender": 2023,
			"recipient": 2001,
			"max_capacity": 8,
			"max_message_size": 512
		},
		{
			"sender": 2001,
			"recipient": 2023,
			"max_capacity": 8,
			"max_message_size": 512
		}
	]
}
